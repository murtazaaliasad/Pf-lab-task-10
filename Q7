#include <stdio.h>
#include <string.h>

int main() {
    char students[4][20];
    char courses[4][10][20];
    int courseCount[4];
    int i, j;
    char searchCourse[20];

    for (i = 0; i < 4; i++) {
        printf("Enter name of student %d: ", i + 1);
        scanf("%s", students[i]);

        printf("Enter number of courses for %s: ", students[i]);
        scanf("%d", &courseCount[i]);

        for (j = 0; j < courseCount[i]; j++) {
            printf("Enter course %d for %s: ", j + 1, students[i]);
            scanf("%s", courses[i][j]);
        }
        printf("\n");
    }

    printf("---- Students and Their Courses ----\n");
    for (i = 0; i < 4; i++) {
        printf("%s: ", students[i]);
        for (j = 0; j < courseCount[i]; j++) {
            printf("%s ", courses[i][j]);
        }
        printf("\n");
    }

    printf("\nEnter course to search for: ");
    scanf("%s", searchCourse);

    int found = 0;
    printf("\nStudents taking %s:\n", searchCourse);
    for (i = 0; i < 4; i++) {
        for (j = 0; j < courseCount[i]; j++) {
            if (strcmp(courses[i][j], searchCourse) == 0) {
                printf("%s\n", students[i]);
                found = 1;
            }
        }
    }
    if (!found) {
        printf("No student is taking this course.\n");
    }

    printf("\n---- Overloaded Students (More than 3 courses) ----\n");
    for (i = 0; i < 4; i++) {
        if (courseCount[i] > 3) {
            printf("%s is overloaded with %d courses.\n", students[i], courseCount[i]);
        }
    }

    return 0;
}
